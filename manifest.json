{
    "manifest_version": 3,
    "name": "Blind Accessibility Helper",
    "version": "1.0",
    "description": "Enhances accessibility for blind users.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  